{
  "name": "express-app",
  "version": "0.1.0",
  "description": "A simple Express application with TypeScript, Prisma, and Zod",
  "keywords": [],
  "author": "Fatih Kaan Akkus",
  "license": "UNLICENSED",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc && tsc-alias",
    "test": "vitest",
    "dev": "ts-node-dev -r tsconfig-paths/register src/app.ts",
    "debug": "ts-node-dev --inspect -r tsconfig-paths/register src/app.ts",
    "preview": "ts-node -r tsconfig-paths/register src/app.ts",
    "db:deploy": "npx prisma migrate deploy && npx prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:format": "prisma format",
    "prisma:dbpush": "prisma db push",
    "prisma:dbseed": "prisma db seed",
    "prisma:generate": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register src/database/seeders/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "cache-manager": "^6.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "prisma": "^6.19.0",
    "prom-client": "^15.1.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.76",
    "zod-validation-error": "^3.5.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.2",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
